{
  "name": "TwitterXDownload",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cloudflare": "./build-cloudflare.sh",
    "start": "next start",
    "lint": "next lint",
    "cf:dev": "wrangler pages dev out --compatibility-date=2024-06-24",
    "cf:deploy": "wrangler pages deploy out",
    "cf:db:create": "wrangler d1 create twitterxdownload",
    "cf:db:migrate": "wrangler d1 execute twitterxdownload --file=./schema.sql"
  },
  "dependencies": {
    "@heroui/react": "^2.7.9",
    "@heroui/theme": "^2.4.16",
    "@remixicon/react": "^4.6.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.15.0",
    "mongoose": "^8.15.1",
    "next": "14.1.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remixicon": "^4.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "wrangler": "^3.0.0"
  }
}
